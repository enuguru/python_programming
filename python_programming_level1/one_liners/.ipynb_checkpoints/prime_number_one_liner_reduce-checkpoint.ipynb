   "source": [
    "from functools import reduce\n",
    "primes = lambda n: reduce( (lambda r,x: r-set(range(x**2,n,x)) if (x in r) else r), range(2,int(n**0.5)), set(range(2,n)))\n",
    "print(primes(int(input())), end = \" \")"
